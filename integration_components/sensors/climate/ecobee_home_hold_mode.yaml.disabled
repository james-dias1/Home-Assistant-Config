---
platform: template
sensors:
  ecobee_home_hold_mode:
    friendly_name: "Temp Hold Mode"
    icon_template: >-
      {% if is_state('states.climate.home.attributes.away_mode', 'on') %}
        mdi:airplane-takeoff
      {% else %}
        mdi:airplane-landing
      {% endif %}
    value_template: "{{ states.climate.home.attributes.away_mode }}"
